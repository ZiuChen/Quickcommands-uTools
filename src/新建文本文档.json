{"program":"quickcommand","cmd":"/*\n  新建文本文档\n  @ZiuChen\n  v0.0.1\n  MIT License\n*/\nconst type = \"txt\" // 保存文档的后缀\nconst fileName = new Date().valueOf() // 文件名 默认为当前时间戳\nconst openAfterCreated = true // 创建成功后是否打开\nconst fs = require(\"fs\");\nutools.readCurrentFolderPath().then((dir) => {\n    let filePath = `${dir}\\\\${fileName}.${type}`\n    fs.writeFile(filePath, \"\", err => {\n        if (err) {\n            utools.showNotification(err)\n        } else {\n          if(openAfterCreated) {\n            open(filePath)\n          }\n        }\n    })\n})","scptarg":"","charset":{"scriptCode":"","outputCode":""},"customOptions":{"bin":"","argv":"","ext":""},"features":{"code":"window_7099hgl91z","explain":"新建文本文档","cmds":[{"type":"window","label":"新建文本文档","match":{"app":["explorer.exe"]}}],"icon":"data:image/png;base64,","platform":["win32"]},"output":"ignore","hasSubInput":false,"tags":[]}